{
  "name": "react-render-log",
  "private": false,
  "version": "0.0.1",
  "description": "React render-log. This package logs the rendering of components in console by debug and highlights the extra rendering.",
  "author": "Kirilenko",
  "license": "MIT",
  "repository": "https://github.com/kirilenko/react-render-log.git",
  "homepage": "https://github.com/kirilenko/react-render-log#readme",
  "engines": {
    "node": "18",
    "pnpm": "7"
  },
  "types": "./build/index.d.ts",
  "type": "module",
  "main": "./build/render-log.umd.js",
  "module": "./build/render-log.es.js",
  "exports": {
    ".": {
      "import": "./build/render-log.es.js",
      "require": "./build/render-log.umd.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "setup": "./scripts/nvm-use.sh && pnpm i",
    "dev": "vite --port 3333",
    "lint:styles": "npx stylelint --fix 'src/**/**.{css,style.ts,style.tsx}'",
    "lint:scripts": "eslint --fix 'src/**/**.{ts,tsx}' './.*.cjs' './*.{cjs,ts}'",
    "format": "pnpm lint:styles && pnpm lint:scripts && prettier --write ./*.json",
    "test": "vitest",
    "test:ci": "vitest --run",
    "prebuild": "rimraf ./build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "pnpm build && npm publish --access public"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12",
    "@testing-library/user-event": "^12",
    "@types/node": "18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "happy-dom": "^8.9.0",
    "postcss": "^8.4.21",
    "postcss-styled-syntax": "0.3.1",
    "prettier": "^2.6.2",
    "rimraf": "^4.4.0",
    "stylelint": "^15.2.0",
    "stylelint-config-css-modules": "4.2.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-config-recommended-scss": "9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.2",
    "stylelint-order-config-standard": "^0.1.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.4.1",
    "vitest": "^0.29.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "keywords": [
    "render-log",
    "react",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/kirilenko/react-render-log/issues"
  }
}
